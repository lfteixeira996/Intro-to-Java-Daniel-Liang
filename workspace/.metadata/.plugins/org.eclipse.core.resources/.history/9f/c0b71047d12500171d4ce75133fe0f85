
public class Exercise1_4 {
	
	public static void main(String[] args)
	{
		System.out.println("a\ta^2\ta^3");
		for (int i = 1; i <= 4; i++) {
			
			System.out.print(i);
			System.out.print("\t");
			System.out.print(i^2);
			System.out.print("\t");
			System.out.print(i^3);
			System.out.println("\t");	
			
		}
	}
}
