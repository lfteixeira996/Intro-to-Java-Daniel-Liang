
public class Exercise1_3 {

	public static void main(String[] args)
	{
		String x1 = String.format("%" + 12 + "s", "V     V"); 
		String x2 = String.format("%" + 10 + "s", " V   V"); 
		String x3 = String.format("%" + 8  + "s", "  V V"); 
		String x4 = String.format("%" + -4 + "s", "   V");
		
		System.out.printf("%5c %6c %s\n",'J', 'A',x1);
		System.out.printf("%5c %7s %s\n",'J', "A A", x2);
		System.out.printf("%c%4c %8s %s\n",'J', 'J',"AAAAA", x3);
		System.out.printf("%2c%2c %10s %s\n",'J', 'J', "A     A", x4);
		
		
		 

		
	}
}
