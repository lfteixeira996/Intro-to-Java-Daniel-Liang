
public class Exercise_01_07 {
	
	public static void main(String[] args)
	{
		double x, y;
		
		x =  4*(1-(1.0/3)+(1.0/5)-(1.0/7)+(1.0/9)-(1.0/11));
		y =  4*(1-(1.0/3)+(1.0/5)-(1.0/7)+(1.0/9)-(1.0/11)+(1.0/13));
		System.out.print("x = ");
		System.out.println(x);
		
		System.out.print("y = ");
		System.out.print(y);
		
	}
}
